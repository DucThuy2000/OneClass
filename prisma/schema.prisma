// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model courses {
  id                   String        @id @default(auto()) @map("_id") @db.ObjectId
  teacher            String?
  createdAt     DateTime?
  status String?
  startAt DateTime?
  endAt DateTime?
}
